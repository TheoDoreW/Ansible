#!/bin/bash
#
sudo supervisorctl status $1|grep days >/dev/null 2>&1
if [ $? -eq 0 ];then
		echo 100;
	else
		time=`sudo supervisorctl status $1|awk '{print $NF}'`
		H=`echo $time |awk -F":" '{print $1}'`
		M=`echo $time |awk -F":" '{print $2}'`
		S=`echo $time |awk -F":" '{print $3}'`
		M1=`expr $H \* 60`
		M2=$M
		RTIME=`expr $M1 + $M2`
		echo $RTIME
fi 
